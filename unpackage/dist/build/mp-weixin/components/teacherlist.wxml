<view class="courselist"><block wx:for="{{video}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['jump',[item.srid?item.srid:item.maid]]]]]}}" class="courselist_box" bindtap="__e"><view class="img"><image src="{{datacourse?item.video_icon:item.video_icon}}" mode="aspectFill"></image><block wx:if="{{!notime}}"><view class="classTime">{{''+(datacourse?'浏览'+item.watch_number:item.video_time?item.video_time:'23:46')+''}}</view></block></view><text class="title">{{item.title}}</text><view class="teach"><text>{{(datacourse?'':item.teachername)+" "+(item.subject?item.subject:item.subjectname)}}</text></view><view class="grade"><text>{{item.gids?item.gids:item.subjectname}}</text><block wx:if="{{istype}}"><view class="btn">预约</view></block></view></view></block></view>