<view class="serch"><view class="serchwrap"><view class="serch_box"><input type="text" placeholder="请输入搜索关键词" confirm-type="search" data-event-opts="{{[['confirm',[['serchdata',['$event']]]],['input',[['__set_model',['','keyword','$event',[]]]]]]}}" value="{{keyword}}" bindconfirm="__e" bindinput="__e"/><view data-event-opts="{{[['tap',[['serchdata',['$event']]]]]}}" bindtap="__e"><image src="../../static/img/shaixuan.png"></image><text>筛选</text></view></view><view class="tag"><block wx:for="{{typelabel}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['activetype',['$0',index],[[['typelabel','',index,'name']]]]]]]}}" class="{{[(item.active=='active')?'active':'']}}" bindtap="__e">{{item.name}}</text></block></view></view><block wx:if="{{nodata}}"><text style="position:absolute;top:30%;left:50%;transform:translateX(-50%);">未搜索到相关内容</text></block><block wx:if="{{isshow}}"><view class="listbot"><view class="courselist"><block wx:for="{{video}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['videojump',['$0'],[[['video','',index,'maid']]]]]]]}}" class="courselist_box" bindtap="__e"><view class="img"><image src="{{item.video_icon}}" mode="aspectFill"></image><view class="classTime">23:46</view></view><text class="title">{{item.title}}</text><view class="teach"><text>{{item.teachername+" "+(item.subject?item.subject:item.subjectname)}}</text></view><view class="grade"><text>{{item.gids?item.gids:item.subjectname}}</text></view></view></block></view><view class="studentlist"><block wx:for="{{student}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['studentjump',['$0'],[[['student','',index,'stid']]]]]]]}}" class="courselist_box" bindtap="__e"><view class="img"><image src="{{item.stu_icon}}" mode="aspectFill"></image></view><text class="title">{{item.stu_name}}</text><view class="teach"><text>{{item.gradename+" | "+item.classname}}</text></view><view class="grade"><text>{{"推荐理由："+item.recommand}}</text></view></view></block></view><view class="courselist"><block wx:for="{{ointment}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['ointmentjump',['$0'],[[['ointment','',index,'srid']]]]]]]}}" class="courselist_box" bindtap="__e"><view class="img"><image src="{{item.video_icon}}" mode="aspectFill"></image></view><text class="title">{{item.title}}</text><view class="teach"><text>{{item.teachername+" "+(item.subject?item.subject:item.subjectname)}}</text></view><view class="grade"><text>{{item.gids?item.gids:item.subjectname}}</text><view class="btn">预约</view></view></view></block></view><view class="courselist"><block wx:for="{{allsubject}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['allsubjectjump']]]]}}" class="courselist_box" bindtap="__e"><view class="img"><image src="{{item.video_icon}}" mode="aspectFill"></image><block wx:if="{{!notime}}"><view class="classTime">{{"浏览"+item.watch_number}}</view></block></view><text class="title">{{item.title}}</text><view class="teach"><text>{{item.subject?item.subject:item.subjectname}}</text></view><view class="grade"><text>{{item.gids?item.gids:item.subjectname}}</text></view></view></block></view><view class="content_wrap"><block wx:for="{{activitylist}}" wx:for-item="item" wx:for-index="__i0__" wx:key="anid"><view data-event-opts="{{[['tap',[['activitylistjump',['$0'],[[['activitylist','anid',item.anid,'anid']]]]]]]}}" bindtap="__e"><image src="{{item.icon}}" mode="aspectFill"></image><view class="text"><text>{{item.title}}</text><text>{{"活动时间："+item.activity_time}}</text></view></view></block></view><view class="subjectlist"><block wx:for="{{subject}}" wx:for-item="item" wx:for-index="__i1__" wx:key="maid"><text data-event-opts="{{[['tap',[['subjectjump',['$0',0],[[['subject','maid',item.maid,'maid']]]]]]]}}" bindtap="__e">{{item.title}}</text></block><block wx:for="{{question_rank}}" wx:for-item="item" wx:for-index="__i2__" wx:key="maid"><text data-event-opts="{{[['tap',[['subjectjump',['$0',3],[[['question_rank','maid',item.maid,'maid']]]]]]]}}" bindtap="__e">{{item.title}}</text></block></view></view></block></view>